<body>

<IFRAME id="childIframe" name="childIframe" class="ui-layout-center" scrolling="auto"
        src=""></IFRAME>

<DIV class="ui-layout-west" style="background-color:rgb(26,24,23) ">
    <div id="menu" style="height: 900px">
        <nav>
        <ul>
            <% rootmenu.forEach(function(t) { %>

            <% if( filterroot == 0) {%>
            <li>
                <a href="/report/allproject/<%- t.info.id %>" target="childIframe"> <%- t.info.name %> </a>
                <ul>
                    <% t.subMenuList.forEach(function(prjList) { %>
                    <li id="area-<%- prjList.info.id %>" >
                        <a href="/report/subproject/<%- prjList.info.id %>" target="childIframe"> <%- prjList.info.name %> </a>
                        <ul>
                            <% prjList.subMenuList.forEach(function(prjInfo) { %>
                            <li>
                                <a href="/report/project/<%- prjInfo.info.id %>" target="childIframe"> <%- prjInfo.info.name %> </a>
                            </li>
                            <% }); %>
                        </ul>
                    </li>
                    <% }); %>
                </ul>
            </li>
            <% } %>



            <% if( filterroot > 0) {%>
                <% if( t.info.id == filterroot) {%>
                        <li><a href="/report/allproject/<%- t.info.id %>" target="childIframe">
                                <i class="fa fa-lg fa-fw fa-home"></i>
                                <span class="menu-item-parent"><%- t.info.name %></span>
                            </a>
                        </li>
                        <% t.subMenuList.forEach(function(prjList) { %>
                        <li id="area-<%- prjList.info.id %>" >

                            <% if(filterroot==24){ %>
                                <a href="/report/subproject/<%- prjList.info.id %>" target="childIframe">
                                    <i class="fa fa-lg fa-fw fa-cube txt-color-blue"></i>
                                    <span ><%- prjList.info.name %></span>
                                </a>
                            <% } else{ %>
                                <a href="#" target="childIframe">
                                    <i class="fa fa-lg fa-fw fa-cube txt-color-blue"></i>
                                    <span class="menu-item-parent"><%- prjList.info.name %></span>
                                </a>
                            <% } %>

                            <% if(filterroot!=24){ %>
                                <ul>
                                    <li><a href="/report/subproject/<%- prjList.info.id %>" target="childIframe">
                                            <span class="menu-item-parent"><%- prjList.info.name %></span>
                                        </a>
                                    </li>class="menu-item-parent"><%- prjList.info.name %></span>

                                    <% prjList.subMenuList.forEach(function(prjInfo) { %>
                                    <li>
                                        <a href="/report/project/<%- prjInfo.info.id %>" target="childIframe"> <%- prjInfo.info.name %> </a>
                                    </li>
                                    <% }); %>
                                </ul>
                            <% } %>

                        </li>
                        <% }); %>
                <% } %>
            <% } %>

            <%  }); %>
        </ul>
        </nav>
    </div>
</DIV>


